            "TF_HTTP_ADDRESS": "https://localhost",
            "TF_HTTP_UPDATE_METHOD": "POST",
            "TF_HTTP_LOCK_ADDRESS": "https://localhost",
            "TF_HTTP_LOCK_METHOD": "POST",
            "TF_HTTP_UNLOCK_ADDRESS": "https://localhost",
            "TF_HTTP_UNLOCK_METHOD": "POST"
        }
    }
